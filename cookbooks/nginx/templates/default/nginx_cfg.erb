server {
    listen <%= @nginx_port %>;
    server_name  nginx;

location /jenkins {
	proxy_pass http://127.0.0.1:<%= @jenkins_port %>/jenkins;
  }
 
    location /mnt-lab {
	proxy_pass	http://127.0.0.1:<%= @tomcat_port %>/mnt-lab;
	}


    error_page  404              /404.html;
    location = /404.html {
        root   /usr/share/nginx/html;
    }


    error_page   500 502 503 504  /50x.html;
    location = /50x.html {
        root   /usr/share/nginx/html;
    }

}
